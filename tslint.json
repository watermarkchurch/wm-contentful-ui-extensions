{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "rules": {
    "semicolon": [true, "never"],
    "quotemark": [true, "single", "jsx-double"],
    "triple-equals": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "no-console": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules",
      "dist",
      "docs"
    ]
  }
}
