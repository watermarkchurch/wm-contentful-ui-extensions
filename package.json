  "description": "Watermark's library of Contentful UI extensions",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.production.js",
    "version": "yarn run clean && webpack --config webpack.production.js && git add -f docs/**/* && git add -f docs/*",
    "serve": "webpack-dev-server -d --config ./webpack.config.js --https",
    "enable-dev": "bin/contentful dev",
    "test": "NODE_ENV=test karma start",
    "test-watch": "NODE_ENV=test karma start --auto-watch --no-single-run",
    "check-types": "tsc --noemit",
    "lint": "tslint src/**/*.ts?\\(x\\)",
    "fix": "tslint src/**/*.ts?\\(x\\) --fix",
    "clean": "rm -r dist; git checkout -- docs; true",
    "prepublishOnly": "yarn run clean && yarn run build"
  },
  "author": "",
  "license": "MIT",
